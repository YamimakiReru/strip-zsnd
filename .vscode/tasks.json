{
  "version": "2.0.0",
  "inputs": [
    {
      "id": "args",
      "type": "promptString",
      "description": "Enter arguments",
      "default": "--help",
    },
  ],
  "tasks": [
    {
      "label": "web_dev",
      "dependsOn": [
        "vite",
        "web_dev_py",
      ],
    },
    {
      "hide": true,
      "label": "web_dev_py",
      "type": "shell",
      "command": ".venv/Scripts/python strip-zsnd.py --debug webui",
      "options": {
        "cwd": "${workspaceFolder}",
      },
    },
    {
      "label": "dev",
      "type": "shell",
      "command": ".venv/Scripts/python strip-zsnd.py --debug ${input:args}",
      "options": {
        "cwd": "${workspaceFolder}",
      },
    },
    {
      "label": "vite",
      "type": "shell",
      "command": "npm run dev",
      "options": {
        "cwd": "${workspaceFolder}/frontend",
      },
      "isBackground": true,
    },
  ],
}